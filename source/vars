# Vagrant
export VAGRANT_DEFAULT_PROVIDER="vmware_fusion"
export VAGRANT_VMWARE_FUSION_APP="/Applications/VMware Fusion.app"

# ansible settings
export ANSIBLE_COW_SELECTION=tux

export DEVLOCAL=/usr/local

# User configuration
export EDITOR=subl
export PATH="$DEVLOCAL/bin:$DEVLOCAL/sbin:$DEVLOCAL/opt/ruby/bin:$HOME/.rvm/bin:$PATH:/usr/bin:/bin:/usr/sbin:/sbin"
export GOROOT=$DEVLOCAL/go
export PATH=$PATH:$GOROOT/bin
