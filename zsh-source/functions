stutopia() {
    export UTOPIA_API=http://utopia-deploy.prv.inf.utopia.com
	export AUTHORIA_API=http://utopia-auth.prv.inf.utopia.com
    export PUBLISH_API=http://utopia-publish.prv.inf.utopia.com
	export TASK_API=http://utopia-tasks.prv.inf.utopia.com
	utopia $@
	unset UTOPIA_API
	unset AUTHORIA_API
	unset TASK_API
	unset PUBLISH_API
}

loutopia() {
	docker=$(dm ip boot2docker)
    export UTOPIA_API=http://${docker}:3535
	export AUTHORIA_API=http://${docker}:3636
    export PUBLISH_API=http://${docker}:3737
	export TASK_API=http://${docker}:3838
	utopia $@
	unset UTOPIA_API
	unset AUTHORIA_API
	unset TASK_API
	unset PUBLISH_API
}

dlogs() {
	local name="$1"
	local id=$(docker ps | grep $name | awk '{print $1}')
	docker logs -f $id
}
